{
	"version": "1",
	"metadata": {
		"name": "Molstar Molecule Viewer",
		"description": "https://github.com/metapages/metaframe-molstar"
	},
	"inputs": {
		"pdb-id": {
			"type": "pdb/id",
			"description": "PDB id https://www.rcsb.org/"
		},
		"target.xtc": {
			"type": "gromacs/xtc",
			"description": "gromacs trajectory file"
		},
		"target.gro": {
			"type": "gromacs/gro",
			"description": "gromacs topology file"
		},
		"target.cif": {
			"type": "Protein Data Bank/CIF"
		},
		"config": {
			"type": "molstar/config",
			"description": "https://github.com/molstar/pdbe-molstar/wiki/1.-PDBe-Molstar-as-JS-plugin"
		}
	},
	"hashParams": [
		"config",
		"pdbId",
		"pdbUrl",
		"cifUrl",
		"xtcUrl",
		"groUrl"
	]
}
